#include <iostream>

using namespace std;

int main()
{
    int num=0;
    int desc=0;
    int art=0;
    cout << "Ingrese el precio: ";
    cin >> num;
    desc= num * 20 / 100 ;
    art= num - desc ;
    if(num > 10000)
    cout << "El total a pagar es: " << art << endl;
    else
    if(num <= 10000)
    cout << "El total a pagar es: " << num << endl;
    return 0;
}
