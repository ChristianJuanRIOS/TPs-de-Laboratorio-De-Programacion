#include <iostream>

using namespace std;

int main() {
    double suma = 0;
    double numero;

    for (int i = 0; i < 10; i++) {
        cout << "Ingrese un número: ";
        cin >> numero;
        suma += numero;
    }

    double promedio = suma / 10;

    cout << "El promedio de los números ingresados es: " << promedio << endl;

    return 0;
}
