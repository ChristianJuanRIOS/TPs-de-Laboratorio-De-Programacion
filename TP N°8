#include <iostream>

using namespace std;

int main()
{
    int A;
    int suma=0;
    int resta=0;
    cout<<"Ingrese A: ";
    cin>>A;
    int B;
    cout<<"Ingrese B: ";
    cin>>B;
    suma= A + B;
    cout<<"La suma es: "<<suma<<endl;
    resta= A - B;
    cout<<"La resta es: "<<resta<<endl;

    return 0;
}
